{
  "name": "ootr-electron-gui",
  "description": "GUI for Ocarina of Time Randomizer",
  "version": "6.1.0",
  "homepage": "https://www.ootrandomizer.com",
  "author": "ZeldaSpeedRuns <zsrstaff@gmail.com>",
  "main": "electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng-dev": "ng serve --configuration hmr",
    "ng-prod": "ng build --configuration production --base-href ./",
    "ng-release": "ng build --base-href ./",
    "electron-compile": "tsc --p electron",
    "electron-dev": "electron .",
    "electron-release": "electron . release",
    "pack": "tsc --p electron && electron-builder --dir",
    "dist": "tsc --p electron && electron-builder"
  },
  "build": {
    "appId": "com.ootrandomizer.electrongui",
    "productName": "Ocarina of Time Randomizer",
    "copyright": "Copyright Â© 2020 ${author}",
    "asar": false,
    "directories": {
      "output": "installer"
    },
    "files": [
      "**/*",
      "!src/*",
      "!bin/*",
      "!obj/*",
      "!e2e/*",
      "!webTest/*",
      "!dist/index.json",
      "!electron/src/*.ts",
      "!electron/tsconfig.json",
      "!electron/dist/index.json",
      "!electron/dist/*.map",
      "!*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj,bat,njsproj,md}",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj,bat,njsproj,md}",
      "!.editorconfig",
      "!angular.json",
      "!tsconfig.json",
      "!tslint.json",
      "!run.js",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes,.bat,.sln,.njsproj,.md}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "python/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "deleteAppDataOnUninstall": false,
      "allowToChangeInstallationDirectory": false,
      "menuCategory": true
    },
    "linux": {
      "target": "tar.gz",
      "executableName": "oot-randomizer-gui"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "internetEnabled": true
    }
  },
  "engines": {
    "node": ">=12 <=15"
  },
  "dependencies": {
    "commander": "2.20.0",
    "electron-window-state": "5.0.3",
    "post-robot": "https://github.com/dragonbane0/post-robot/tarball/dad59c1c0f6266d0dd4bd35e0fb7137d45c6c773",
    "tree-kill": "1.2.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular-eslint/eslint-plugin": "^14.1.2",
    "@angular/animations": "12.2.16",
    "@angular/cdk": "12.2.13",
    "@angular/cli": "12.2.18",
    "@angular/common": "12.2.16",
    "@angular/compiler": "12.2.16",
    "@angular/compiler-cli": "12.2.16",
    "@angular/core": "12.2.16",
    "@angular/flex-layout": "12.0.0-beta.35",
    "@angular/forms": "12.2.16",
    "@angular/language-service": "12.2.16",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "12.2.16",
    "@angular/platform-browser-dynamic": "12.2.16",
    "@angular/router": "12.2.16",
    "@angularclass/hmr": "3.0.0",
    "@fortawesome/fontawesome-free": "6.2.0",
    "@nebular/eva-icons": "8.0.0",
    "@nebular/theme": "8.0.0",
    "@types/electron": "1.6.10",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "angular-dual-listbox": "4.7.0",
    "angular-file": "3.6.0",
    "browserslist": "^4.21.4",
    "core-js": "3.25.1",
    "crc": "3.8.0",
    "electron": "4.2.6",
    "eslint": "^8.25.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.14",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eva-icons": "1.1.3",
    "hammerjs": "2.0.8",
    "intl": "1.2.5",
    "ionicons": "4.6.3",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jquery": "3.4.1",
    "karma": "6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ngx-color-picker": "https://github.com/dragonbane0/ngx-color-picker/tarball/7471e8595e5bf508813c5f6b1964e53ac9902448",
    "normalize.css": "6.0.0",
    "pace-progress": "https://github.com/dragonbane0/pace/tarball/bcbd25563bf6193b223f856dc1668c0705fd8710",
    "popper.js": "1.16.1",
    "protractor": "7.0.0",
    "roboto-fontface": "0.8.0",
    "rxjs": "6.6.7",
    "rxjs-compat": "6.3.0",
    "sass": "^1.55.0",
    "socicon": "3.0.5",
    "tcp-ping": "0.1.1",
    "ts-node": "7.0.1",
    "tslint": "6.1.3",
    "typeface-exo": "0.0.22",
    "typescript": "4.3.5",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "overrides": {
    "browserslist": {
      "caniuse-lite": "1.0.30001393"
    },
    "autoprefixer": {
      "caniuse-lite": "1.0.30001393"
    }
  }
}
